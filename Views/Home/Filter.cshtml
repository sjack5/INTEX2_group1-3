@{
}

@model SearchViewModel

<table>
    <thead>Sex</thead>
    <td>
        @foreach (string sex in Model.Burials.Select(b => b.Sex))
        {
            <option value="@sex">@sex</option>
        }
    </td>
    <thead>Depth</thead>
    <td>
        @foreach (string depth in Model.Burials.Select(b => b.Depth))
        {
            <option value="@depth">@depth</option>
        }
    </td>
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
</table>
